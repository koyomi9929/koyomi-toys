<h1>NEW</h1>

<%= form_for @toy do |f| %>
  <%= f.label "名前" %><br>
  <%= f.text_field :name, class: "form_control" %> <br>
  <%= f.label "ジャンル" %><br>
  <%= f.text_field :genre, class: "form_control" %> <br>
  <%= f.label "詳細" %><br>
  <%= f.text_field :detail, class: "form_control" %> <br>
  <%= f.label "Controlelr名" %><br>
  <%= f.text_field :path, class: "form_control" %> <br>
  <%= f.label "画像（URL）" %><br>
  <%= f.text_field :image, class: "form_control" %> <br>
  <%= f.submit "作成"%>
<% end %>
